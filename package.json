{
  "name": "CryptoCalc",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "git@github.com:Stogniev/CryptoCalc.git",
  "author": "Alex_Y <yegupov@gmail.com>",
  "license": "",
  "scripts": {
    "test": "(cd demo-site/src/demo && ./build-and-test2.sh)",
    "test-debug": "(cd demo-site/src/demo && DEBUG=1 ./build-and-test2.sh)",
    "console-debug": "(cd demo-site/src/demo && DEBUG=1 ./build-and-test2.sh)",
    "start": "(cd ./demo-site && yarn run start)",
    "build": "(cd ./demo-site && npm run build && rm -rf ../docs/* && cp -rf build/* ../docs/ && cd .. && sed -i 's/\\/static/static/g' docs/index.html && git add docs/static/js/\\*.js)",
    "build-and-run": "npm run build && npm run localServer",
    "localServer": "(cd docs && python -m SimpleHTTPServer 9000)",
    "firebase-deploy-functions-node6": "(cd firebase/functions && firebase deploy --only functions)",
    "firebase-deploy-functions": "(cd firebase/functions && npx -p node-bin@6.11 -- firebase deploy --only functions)",
    "firebase-shell": "(cd firebase/functions && GOOGLE_APPLICATION_CREDENTIALS=../cryptocalc1-d51fa5762f45.json firebase experimental:functions:shell)"
  },
  "dependencies": {
    "escape-string-regexp": "^1.0.5",
    "is": "^3.2.1",
    "moo": "^0.4.3",
    "prop-types": "^15.6.0",
    "react-helmet": "^5.2.0"
  }
}
